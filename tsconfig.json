{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules",
    "fluffyfi-plaid-link"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./*"
      ]
    },
    "baseUrl": ".",
    "verbatimModuleSyntax": false,
    "strictNullChecks": true,
  }
}
